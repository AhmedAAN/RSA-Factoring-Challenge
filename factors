#!/usr/bin/python3v
import sys
import math


with open(sys.argv[1], mode = "r") as file:
    lines = file.readlines()
for line in lines:
    number = int(line)
    divisor = 0
    if number %2 == 0:
        divisor = 2
    elif number %3 ==0:
        divisor = 3
    else:
        i = 6
        while i <= int(math.sqrt(number) + 1) and divisor == 0:
            if number % (i - 1) == 0:
                divisor = i - 1
            if number % (i + 1) == 0:
                divisor = i + 1
            i += 6
        if divisor == 0:
            divisor = 1
    print(f"{number}={int(number/divisor)}*{divisor}")
